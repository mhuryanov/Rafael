    "test:watch": "jest --watch front_end/__tests__",
    "test:coverage": "jest --coverage --colors front_end/__tests__",
    "test:update": "jest front_end/__tests__ --update-snapshot",
    "serve": "http-server",
    "start": "webpack-dev-server"
  },
